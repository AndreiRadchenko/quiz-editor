{
  "expo": {
    "name": "Quiz Player",
    "slug": "quizPlayer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "statusBarHidden": true,
    "backgroundColor": "#1a1a1a",
    "splash": {
      "image": "./assets/splash-logo-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a1a1a",
      "dark": {
        "image": "./assets/splash-logo-dark.png",
        "resizeMode": "cover",
        "backgroundColor": "#1a1a1a"
      }
    },
    "ios": {
      "supportsTablet": true,
      "statusBarHidden": true,
      "backgroundColor": "#1a1a1a",
      "splash": {
        "image": "./assets/splash-logo-dark.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a",
        "tabletImage": "./assets/splash-logo-dark.png",
        "dark": {
          "image": "./assets/splash-logo-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a",
          "tabletImage": "./assets/splash-logo-dark.png"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#1a1a1a"
      },
      "backgroundColor": "#1a1a1a",
      "splash": {
        "image": "./assets/splash-logo-dark.png",
        "resizeMode": "cover",
        "backgroundColor": "#1a1a1a",
        "mdpi": "./assets/splash-logo-dark.png",
        "hdpi": "./assets/splash-logo-dark.png",
        "xhdpi": "./assets/splash-logo-dark.png",
        "xxhdpi": "./assets/splash-logo-dark.png",
        "xxxhdpi": "./assets/splash-logo-dark.png",
        "dark": {
          "image": "./assets/splash-logo-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#1a1a1a",
          "mdpi": "./assets/splash-logo-dark.png",
          "hdpi": "./assets/splash-logo-dark.png",
          "xhdpi": "./assets/splash-logo-dark.png",
          "xxhdpi": "./assets/splash-logo-dark.png",
          "xxxhdpi": "./assets/splash-logo-dark.png"
        }
      },
      "statusBar": {
        "hidden": true,
        "translucent": true,
        "backgroundColor": "#1a1a1a"
      },
      "navigationBar": {
        "visible": false,
        "backgroundColor": "#1a1a1a"
      }
    },
    "web": {
      "favicon": "./assets/logo.png",
      "backgroundColor": "#1a1a1a"
    }
  }
}
