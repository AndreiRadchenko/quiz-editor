{
  "expo": {
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "name": "Quiz Supervisor",
    "slug": "quizSupervisor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-square.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "backgroundColor": "#1a1a1a",
    "splash": {
      "image": "./assets/logo-square.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a1a1a",
      "dark": {
        "image": "./assets/logo-square.png",
        "resizeMode": "cover",
        "backgroundColor": "#1a1a1a"
      }
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#1a1a1a",
      "splash": {
        "image": "./assets/splash-logo-dark.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a",
        "tabletImage": "./assets/splash-logo-dark.png",
        "dark": {
          "image": "./assets/splash-logo-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a",
          "tabletImage": "./assets/splash-logo-dark.png"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-square.png",
        "backgroundColor": "#1a1a1a"
      },
      "backgroundColor": "#1a1a1a",
      "splash": {
        "image": "./assets/logo-square.png",
        "resizeMode": "cover",
        "backgroundColor": "#1a1a1a",
        "dark": {
          "image": "./assets/logo-square.png",
          "resizeMode": "cover",
          "backgroundColor": "#1a1a1a"
        }
      },
      "package": "com.andriiradchenko.quizPlayer"
    },
    "web": {
      "favicon": "./assets/logo-square.png",
      "backgroundColor": "#1a1a1a"
    },
    "extra": {
      "eas": {
        "projectId": "9fd7a24f-e33c-4210-b521-b73cb970ccab"
      }
    }
  }
}
